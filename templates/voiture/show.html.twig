{% extends 'base.html.twig' %}

{% block title %}Voiture{% endblock %}

{% block body %}
    <h1>Voiture</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ voiture.id }}</td>
            </tr>
            <tr>
                <th>Immatriculation</th>
                <td>{{ voiture.immatriculation }}</td>
            </tr>
            <tr>
                <th>Marque</th>
                <td>{{ voiture.marque }}</td>
            </tr>
            <tr>
                <th>Modele</th>
                <td>{{ voiture.modele }}</td>
            </tr>
            <tr>
                <th>Finition</th>
                <td>{{ voiture.finition }}</td>
            </tr>
            <tr>
                <th>DateAchat</th>
                <td>{{ voiture.dateAchat ? voiture.dateAchat|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Energie</th>
                <td>{{ voiture.energie }}</td>
            </tr>
            <tr>
                <th>CvFiscale</th>
                <td>{{ voiture.cvFiscale }}</td>
            </tr>
            <tr>
                <th>CvDin</th>
                <td>{{ voiture.cvDin }}</td>
            </tr>
            <tr>
                <th>Vente</th>
                <td>{{ voiture.vente ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('voiture_index') }}">back to list</a>

    <a href="{{ path('voiture_edit', {'id': voiture.id}) }}">edit</a>

    {{ include('voiture/_delete_form.html.twig') }}
{% endblock %}
